<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jsw.manageproductrecycler">

    <application
        android:name="com.jsw.manageproductrecycler.Product_Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.jsw.manageproductrecycler.SplashScreen_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jsw.manageproductrecycler.Login_Activity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.jsw.manageproductrecycler.ManageProduct_Activity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.jsw.manageproductrecycler.AddProduct_Activity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>